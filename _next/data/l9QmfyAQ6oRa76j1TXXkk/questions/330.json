 entry of entries) {\r\n        // `entry` is a PerformanceEntry instance.\r\n        // `name` will be either 'first-paint' or 'first-contentful-paint'.\r\n        const metricName = entry.name;\r\n        const time = Math.round(entry.startTime + entry.duration);\r\n        // 获得FP，首次渲染事件\r\n        if (metricName === 'first-paint') {\r\n            performanceMetrics.fp = time;\r\n        }\r\n        // 获得FCP，首次文档渲染事件\r\n        if (metricName === 'first-contentful-paint') {\r\n            performanceMetrics.fcp = time;\r\n        }\r\n  }\r\n}\r\nvar observer2 = new PerformanceObserver(perfObserver);\r\nobserver2.observe({entryTypes: [\"paint\"]});\r\n```","closed_by":null,"reactions":{"url":"https://api.github.com/repos/hankliu62/interview/issues/330/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"timeline_url":"https://api.github.com/repos/hankliu62/interview/issues/330/timeline","performed_via_github_app":null,"state_reason":null},"menus":[]},"__N_SSG":true}